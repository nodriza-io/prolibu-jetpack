  "activationEvents": [
    "onCommand:extension.abrirCsv",
    "onCommand:extension.start"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.abrirCsv",
        "title": "PRL: Watch CSV"
      },
      {
        "command": "extension.start",
        "title": "PRL: Start"
      }
    ],
    "configuration": {
      "title": "Configuración de Mi Extensión",
      "properties": {
        "miExtension.userData": {
          "type": "object",
          "default": "",
          "description": "Ruta principal para los archivos CSV."
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.81.0",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "chokidar": "^3.5.3",
    "commander": "^11.0.0",
    "exceljs": "^4.3.0",
    "inquirer": "^9.2.10",
